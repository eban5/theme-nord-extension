{
  "name": "@kenshohara/theme-nord-extension",
  "version": "0.1.0",
  "description": "JupyterLab - Nord Theme",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/kenshohara/theme-nord-extension",
  "bugs": {
    "url": "https://github.com/kenshohara/theme-nord-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Kensho Hara",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kenshohara/theme-nord-extension.git"
  },
  "scripts": {
    "build": "tsc -b",
    "build:src": "tsc -b src",
    "build:webpack": "webpack",
    "clean": "rimraf lib && rimraf static",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch",
    "watch:src": "tsc -b src --watch",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "font-awesome": "~4.7.0"
  },
  "devDependencies": {
    "css-loader": "~0.28.7",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "~4.1.1",
    "rimraf": "~2.6.2",
    "svg-url-loader": "~2.3.1",
    "svgo": "~1.0.4",
    "svgo-loader": "~2.1.0",
    "ts-loader": "~5.0.0",
    "typedoc": "~0.12.0",
    "typescript": "~3.1.1",
    "url-loader": "~1.0.1",
    "webpack": "~4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themeDir": "static"
  }
}
